{
  "name": "discord-cotd-action",
  "version": "1.0.1",
  "private": true,
  "description": "A GitHub Action to modify a role on a Discord guild.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license LICENSE",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@discordjs/collection": "^1.1.0",
    "ms": "^3.0.0-canary.1",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@typescript-eslint/parser": "^5.36.2",
    "@vercel/ncc": "^0.34.0",
    "discord-api-types": "^0.37.9",
    "eslint": "^8.33.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.3"
  }
}
